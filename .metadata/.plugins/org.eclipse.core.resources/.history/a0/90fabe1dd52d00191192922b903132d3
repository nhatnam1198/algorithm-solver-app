
public class Process {
	private int arrivalTime;
	private int burstTime;
	void getArrivalTime(int para) {
		return arrivalTime;
	}
	
	void getBurstTime(int para){
		return burstTime;
	}
	void setArrivalTime(int para) {
		this.arrivalTime = para;
	}
	
	void setBurstTime(int para){
		this.burstTime = para;
	}
}
